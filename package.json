{
  "name": "oauth-node-starter",
  "version": "1.0.0",
  "description": "A UI showcase of social network statistics",
  "main": "app.js",
  "scripts": {
    "start": "SET DEBUG=oauth-node-starter:* & nodemon ./bin/www",
    "production": "SET NODE_ENV=production & node ./bin/www",
    "migrate": "sequelize db:migrate",
    "migration-create": "sequelize migration:create",
    "migrate-undo": "sequelize db:migrate:undo",
    "seed": "sequelize db:seed:all",
    "seed-create": "sequelize seed:generate",
    "seed-undo": "sequelize db:seed:undo:all",
    "lint": "eslint ."
  },
  "repository": "git+https://github.com/praveenkumar-outlook/OAuth-Node.git",
  "keywords": [
    "nodejs",
    "expressjs",
    "sequelize",
    "oauth2",
    "oauth2-server",
    "postgresql",
    "swagger",
    "swagger-ui"
  ],
  "author": "Praveenkumar K <praveen.ece@outlook.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/praveenkumar-outlook/OAuth-Node/issues"
  },
  "homepage": "https://github.com/praveenkumar-outlook/OAuth-Node#readme",
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "morgan": "~1.9.0",
    "oauth2-server": "^3.0.1",
    "password-hash": "^1.2.2",
    "pg": "^7.8.0",
    "sequelize": "^4.42.0",
    "swagger-jsdoc": "^3.2.6",
    "swagger-ui-express": "^4.0.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "eslint": "^5.13.0",
    "nodemon": "^1.18.9",
    "sequelize-cli": "^5.4.0"
  }
}
